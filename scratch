window.t = testShader;
window.x = submerged.filter;
function testShader() {
    for (let unit of window.underworld.units) {
        if (unit && unit.image) {
            unit.shaderUniforms.submerged = submerged.uniforms;
            unit.image.sprite.filters = [submerged.filter];
        }
    }
}


