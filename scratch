for(let i = 0; i < 12; i++){
    const adjustedLevelIndex = i;
  let budgetLeft = ((adjustedLevelIndex + 1) * Math.max(5, (adjustedLevelIndex + 1) - 3));
    console.log('level', i,': budget:', budgetLeft, Math.max(5, (adjustedLevelIndex + 1) - 3));
}
